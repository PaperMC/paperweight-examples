+
+            if (willCreateNewLine) {
+                output.add(alignment.align(builder.toString(), maxChars));
+                builder = new StringBuilder();
+                charCounter = 0;
+            }
+
+            charCounter += word.length();
+            builder.append(word);
+            if (!willCreateNewLine && i != words.length - 1)
+                builder.append(" ");
