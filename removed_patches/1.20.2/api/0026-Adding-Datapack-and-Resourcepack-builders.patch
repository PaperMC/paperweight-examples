+                    pad(where, maxChars - wanted.length());
+                    where.append(wanted);
+                }
+                case CENTER -> {
+                    int toAdd = maxChars - wanted.length();
+                    pad(where, toAdd / 2);
+                    where.append(wanted);
+                    pad(where, toAdd - toAdd / 2);
